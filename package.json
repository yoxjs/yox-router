{
  "name": "yox-router",
  "version": "1.0.0-alpha25",
  "description": "Official Router for Yox.js",
  "main": "dist/yox-router.js",
  "types": "dist/yox-router.d.ts",
  "keywords": [
    "router",
    "mvvm",
    "yoxjs"
  ],
  "scripts": {
    "dev": "node_modules/.bin/rollup -c ./build/config.dev.js",
    "prod": "node_modules/.bin/rollup -c ./build/config.prod.js",
    "dts": "./node_modules/.bin/dts-bundle-generator -o dist/yox-router.d.ts ../dist/yox-router/src/Router.d.ts",
    "doc": "docsify serve ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoxjs/yox-router.git"
  },
  "author": "musicode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoxjs/yox-router/issues"
  },
  "homepage": "https://github.com/yoxjs/yox-router#readme",
  "devDependencies": {
    "rollup": "^1.10.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-filesize": "^6.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2",
    "dts-bundle-generator": "^3.1.0"
  }
